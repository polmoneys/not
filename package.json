  "license": "MIT",
  "homepage": "https://github.com/polmoneys/not",
  "repository": {
    "type": "git",
    "url": "https://github.com/polmoneys/not"
  },
  "bugs": {
    "url": "https://github.com/polmoneys/not/issues"
  },
  "keywords": [
    "aesthetic",
    "ts",
    "not"
  ],
  "main": "./dist/not.umd.cjs",
  "module": "./dist/not.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/not.js",
      "require": "./dist/not.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
